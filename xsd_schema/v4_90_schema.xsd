<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">

    <xs:element name="Sysmon">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="EventFiltering">
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded">
                            <xs:element name="RuleGroup" type="RuleGroupType"/>
                            <xs:element name="ProcessCreate" type="ProcessCreateType"/>
                            <xs:element name="FileCreateTime" type="FileCreateTimeType"/>
                            <xs:element name="NetworkConnect" type="NetworkConnectType"/>
                            <xs:element name="ProcessTerminate" type="ProcessTerminateType"/>
                            <xs:element name="DriverLoad" type="DriverLoadType"/>
                            <xs:element name="ImageLoad" type="ImageLoadType"/>
                            <xs:element name="CreateRemoteThread" type="CreateRemoteThreadType"/>
                            <xs:element name="RawAccessRead" type="RawAccessReadType"/>
                            <xs:element name="ProcessAccess" type="ProcessAccessType"/>
                            <xs:element name="FileCreate" type="FileCreateType"/>
                            <xs:element name="RegistryEvent" type="RegistryEventType"/>
                            <xs:element name="FileCreateStreamHash" type="FileCreateStreamHashType"/>
                            <xs:element name="PipeEvent" type="PipeEventType"/>
                            <xs:element name="WmiEvent" type="WmiEventType"/>
                            <xs:element name="DnsQuery" type="DnsQueryType"/>
                            <xs:element name="FileDelete" type="FileDeleteType"/>
                            <xs:element name="ClipboardChange" type="ClipboardChangeType"/>
                            <xs:element name="ProcessTampering" type="ProcessTamperingType"/>
                            <xs:element name="FileDeleteDetected" type="FileDeleteDetectedType"/>
                            <xs:element name="FileBlockExecutable" type="FileBlockExecutableType"/>
                            <xs:element name="FileBlockShredding" type="FileBlockShreddingType"/>
                            <xs:element name="FileExecutableDetected" type="FileExecutableDetectedType"/>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
                <xs:element name="ArchiveDirectory" type="xs:string" minOccurs="0"/>
                <xs:element name="CaptureClipboard" minOccurs="0">
                    <xs:complexType/>
                </xs:element>
                <xs:element name="DriverName" type="xs:string" minOccurs="0"/>
                <xs:element name="PipeMonitoringConfig" type="xs:string" minOccurs="0"/>
                <xs:element name="HashAlgorithms" type="xs:string" minOccurs="0"/>
                <xs:element name="DnsLookup" type="xs:string" minOccurs="0"/>
                <xs:element name="ProcessAccessConfig" type="xs:string" minOccurs="0"/>
                <xs:element name="CheckRevocation" type="xs:string" minOccurs="0"/>
                <xs:element name="FieldSizes" type="xs:string" minOccurs="0"/>
            </xs:choice>
            <xs:attribute name="schemaversion" type="xs:decimal" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="RuleGroupType">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="ProcessCreate" type="ProcessCreateType"/>
            <xs:element name="FileCreateTime" type="FileCreateTimeType"/>
            <xs:element name="NetworkConnect" type="NetworkConnectType"/>
            <xs:element name="ProcessTerminate" type="ProcessTerminateType"/>
            <xs:element name="DriverLoad" type="DriverLoadType"/>
            <xs:element name="ImageLoad" type="ImageLoadType"/>
            <xs:element name="CreateRemoteThread" type="CreateRemoteThreadType"/>
            <xs:element name="RawAccessRead" type="RawAccessReadType"/>
            <xs:element name="ProcessAccess" type="ProcessAccessType"/>
            <xs:element name="FileCreate" type="FileCreateType"/>
            <xs:element name="RegistryEvent" type="RegistryEventType"/>
            <xs:element name="FileCreateStreamHash" type="FileCreateStreamHashType"/>
            <xs:element name="PipeEvent" type="PipeEventType"/>
            <xs:element name="WmiEvent" type="WmiEventType"/>
            <xs:element name="DnsQuery" type="DnsQueryType"/>
            <xs:element name="FileDelete" type="FileDeleteType"/>
            <xs:element name="ClipboardChange" type="ClipboardChangeType"/>
            <xs:element name="ProcessTampering" type="ProcessTamperingType"/>
            <xs:element name="FileDeleteDetected" type="FileDeleteDetectedType"/>
            <xs:element name="FileBlockExecutable" type="FileBlockExecutableType"/>
            <xs:element name="FileBlockShredding" type="FileBlockShreddingType"/>
            <xs:element name="FileExecutableDetected" type="FileExecutableDetectedType"/>
        </xs:choice>
        <xs:attribute name="groupRelation" type="GroupRelationType" use="required"/>
        <xs:attribute name="name" type="xs:string" use="optional"/>
    </xs:complexType>

    <xs:simpleType name="GroupRelationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="and"/>
            <xs:enumeration value="or"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:attributeGroup name="ruleAttributes">
        <xs:attribute name="onmatch" type="OnMatchType" use="optional"/>
        <xs:attribute name="default" type="DefaultMatchType" use="optional"/>
    </xs:attributeGroup>

    <xs:simpleType name="OnMatchType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="include"/>
            <xs:enumeration value="exclude"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DefaultMatchType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="include"/>
            <xs:enumeration value="exclude"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Base type for rule data elements -->
    <xs:complexType name="RuleDataType" abstract="true">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="condition" type="ConditionType" default="is" use="optional"/>
                <xs:attribute name="name" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="ConditionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="is"/>
            <xs:enumeration value="is not"/>
            <xs:enumeration value="contains"/>
            <xs:enumeration value="contains any"/>
            <xs:enumeration value="is any"/>
            <xs:enumeration value="contains all"/>
            <xs:enumeration value="excludes"/>
            <xs:enumeration value="excludes any"/>
            <xs:enumeration value="excludes all"/>
            <xs:enumeration value="begin with"/>
            <xs:enumeration value="not begin with"/>
            <xs:enumeration value="end with"/>
            <xs:enumeration value="not end with"/>
            <xs:enumeration value="less than"/>
            <xs:enumeration value="more than"/>
            <xs:enumeration value="image"/>
        </xs:restriction>
    </xs:simpleType>


    <!-- Event Type Definitions -  Each event type now extends ruleAttributes and contains its specific data elements -->

    <xs:complexType name="ProcessCreateType">
        <xs:complexContent>
            <xs:extension base="RuleGroupType">
             <xs:sequence>
                    <xs:element name="Rule" type="ProcessCreateRuleType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attributeGroup ref="ruleAttributes"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ProcessCreateRuleType">
