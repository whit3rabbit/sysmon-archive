            <xs:element name="FileVersion" type="FileVersionType" minOccurs="0"/>
            <xs:element name="Description" type="DescriptionType" minOccurs="0"/>
            <xs:element name="Product" type="ProductType" minOccurs="0"/>
            <xs:element name="Company" type="CompanyType" minOccurs="0"/>
