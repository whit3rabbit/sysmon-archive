            <xs:element name="User" type="UserType" minOccurs="0"/>
            <xs:element name="LogonGuid" type="LogonGuidType" minOccurs="0"/>
            <xs:element name="LogonId" type="LogonIdType" minOccurs="0"/>
            <xs:element name="TerminalSessionId" type="TerminalSessionIdType" minOccurs="0"/>
            <xs:element name="IntegrityLevel" type="IntegrityLevelType" minOccurs="0"/>
            <xs:element name="Hashes" type="HashesType" minOccurs="0"/>
            <xs:element name="ParentProcessGuid" type="ParentProcessGuidType" minOccurs="0"/>
            <xs:element name="ParentProcessId" type="ParentProcessIdType" minOccurs="0"/>
            <xs:element name="ParentImage" type="ParentImageType" minOccurs="0"/>
            <xs:element name="ParentCommandLine" type="ParentCommandLineType" minOccurs="0"/>
            <xs:element name="ParentUser" type="ParentUserType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>


