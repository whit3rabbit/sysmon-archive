<?xml version="1.0" encoding="UTF-16"?>
<instrumentationManifest xsi:schemaLocation="http://schemas.microsoft.com/win/2004/08/events eventman.xsd"
                         xmlns="http://schemas.microsoft.com/win/2004/08/events"
                         xmlns:win="http://manifests.microsoft.com/win/2004/08/windows/events"
                         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                         xmlns:xs="http://www.w3.org/2001/XMLSchema"
                         xmlns:trace="http://schemas.microsoft.com/win/2004/08/events/trace">
    <instrumentation>
        <events>
            <provider name="Microsoft-Windows-Sysmon"
                      guid="{5770385F-C22A-43E0-BF4C-06F5698FFBD9}"
                      symbol="SYSMON_PROVIDER"
                      resourceFileName="%filename%"
                      messageFileName="%filename%">
                <events>
                    <event symbol="SYSMON_ERROR_EVENT"
                           value="255"
                           version="1"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Error"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="Error report"
                           message="$(string.event.str_SYSMON_ERROR)" />
                    <event symbol="SYSMON_CREATE_PROCESS_EVENT"
                           value="1"
                           version="3"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="Process Create"
                           message="$(string.event.str_SYSMON_CREATE_PROCESS)" />
                    <event symbol="SYSMON_FILE_TIME_EVENT"
                           value="2"
                           version="2"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="File creation time changed"
                           message="$(string.event.str_SYSMON_FILE_TIME)" />
                    <event symbol="SYSMON_NETWORK_CONNECT_EVENT"
                           value="3"
                           version="3"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="Network connection detected"
                           message="$(string.event.str_SYSMON_NETWORK_CONNECT)" />
                    <event symbol="SYSMON_SERVICE_STATE_CHANGE_EVENT"
                           value="4"
                           version="1"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="Sysmon service state changed"
                           message="$(string.event.str_SYSMON_SERVICE_STATE_CHANGE)" />
                    <event symbol="SYSMON_FILE_DELETE_EVENT"
                           value="61441"
                           version="3"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="File Delete"
                           message="$(string.event.str_SYSMON_FILE_DELETE)" />
                    <event symbol="SYSMON_CLIPBOARD_EVENT"
                           value="61442"
                           version="3"
                           channel="Microsoft-Windows-Sysmon/Operational"
                           level="win:Informational"
                           task="SysmonTask"
                           opcode="win:Info"
                           template="Clipboard changed"
                           message="$(string.event.str_SYSMON_CLIPBOARD)" />
                </events>
                <levels />
                <tasks>
                    <task name="SysmonTask"
                          symbol="SYSMON_TASK"
                          value="1"
                          eventGUID="{C511FFB3-9FBF-45F5-A97B-9BEEFCB2071B}" />
                </tasks>
                <opcodes />
                <channels>
                    <channel name="Microsoft-Windows-Sysmon/Operational"
                             chid="c1"
                             symbol="SYSMON_CHANNEL"
                             type="Operational"
                             enabled="true" />
                </channels>
                <templates>
                    <template tid="Error report">
                        <data name="ID"
                              inType="win:UnicodeString"
                              outType="xs:string" />
                        <data name="Description"
                              inType="win:UnicodeString"
                              outType="xs:string" />
                    </template>
                    <template tid="Process Create">
                        <data name="UtcTime"
                              inType="win:UnicodeString"
                              outType="xs:string" />
                        <data name="ProcessGuid"
                              inType="win:GUID" />
                        <data name="ProcessId"
                              inType="win:UInt32"
                              outType="win:PID" />
                        <data name="Image"
                              inType="win:UnicodeString"
                              outType="xs:string" />
                        <data name="CommandLine"
                              inType="win:UnicodeString"
                              outType="xs:string" />
                        <data name="User"
                              inType="win:UnicodeString"
                              outType="xs:string" />
